{
  "name": "PlantApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "tsc": "tsc --noEmit",
    "generate:image": "img-res-gen --dir=./src/assets/images --out=./src/assets/Generated/ImageResources.g.ts --ts",
    "generate:icons": "svgr ./src/assets/svgs -d ./src/assets/icons --native --icon - --ignore-existing",
    "prepare": "husky install",
    "e2e:test-debug:ios": "rm -rf ./__tests__/artifacts/current && rm -rf ./__tests__/artifacts/diff && detox test --configuration ios.sim.debug --artifacts-location ./artifacts/",
    "e2e:test-release:ios": "detox test --configuration ios.sim.release",
    "e2e:build-debug:ios": "detox build --configuration ios.sim.debug",
    "e2e:build-release:ios": "detox build --configuration ios.sim.release",
    "compare:ui": "IS_DETOX=true yarn run e2e:test-debug:ios && ./compare_images.sh",
    "update:baseline": "rm -rf rm -rf ./__tests__/artifacts/baseline/* && cp ./__tests__/artifacts/current/* ./__tests__/artifacts/baseline"
  },
  "dependencies": {
    "@react-navigation/bottom-tabs": "^6.6.1",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.11.0",
    "@reduxjs/toolkit": "^2.2.7",
    "i18next": "^23.15.1",
    "react": "18.3.1",
    "react-i18next": "^15.0.2",
    "react-native": "0.75.3",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-mmkv": "2.8.0",
    "react-native-safe-area-context": "^4.11.0",
    "react-native-screens": "^3.34.0",
    "react-native-svg": "^15.7.1",
    "react-redux": "^9.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.75.3",
    "@react-native/eslint-config": "0.75.3",
    "@react-native/metro-config": "0.75.3",
    "@react-native/typescript-config": "0.75.3",
    "@svgr/cli": "^8.1.0",
    "@testing-library/react-native": "^12.7.2",
    "@types/jest": "^29.5.13",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "babel-plugin-module-resolver": "^5.0.2",
    "detox": "^20.27.2",
    "eslint": "^8.19.0",
    "eslint-plugin-i18next": "^6.1.0",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-import-alias": "^1.2.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-testing-library": "^6.3.0",
    "husky": "^8.0.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-native-image-resource-generator": "^1.0.2",
    "react-test-renderer": "18.3.1",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@3.6.4"
}
